    </category>
    
      <category>
        <pattern>HELLO</pattern>
        <template>
        <condition name="name">
        <li value="">Hi, nice to meet you! What is your name?</li>
        <li>Hi <get name="name"></get>! How are you today?</li>
        </condition>
        </template>
    </category>

      <category>
